<?xml version="1.0"?>
{# Copy this to any page you want articles data on. #}
{% set data = get('articles') %}

{# This is how you set pagination. #}
{% set data = paginate(data, 10) %}

<rss version="2.0">
  <channel>
    <title>Site name</title>
    <link>http://www.sitename.com</link>
    <description>isdf</description>
    {% for object in data %}
      <item>
        <title>{{ object.name }}</title>
        <link>{{ url(object) }}</link>
        <description>{{ object.deck }}</description>
      </item>
    {% endfor %}
  </channel>
</rss>
